#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yawen.xiao/Codes/demo/astro-docusaurus/crawler/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yawen.xiao/Codes/demo/astro-docusaurus/crawler/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../r2.cnpmjs.org+esprima@4.0.1/node_modules/esprima/bin/esparse.js" "$@"
else
  exec node  "$basedir/../../../../../r2.cnpmjs.org+esprima@4.0.1/node_modules/esprima/bin/esparse.js" "$@"
fi
