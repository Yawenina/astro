#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yawen.xiao/Codes/demo/astro-docusaurus/crawler/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yawen.xiao/Codes/demo/astro-docusaurus/crawler/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../r2.cnpmjs.org+escodegen@2.0.0/node_modules/escodegen/bin/esgenerate.js" "$@"
else
  exec node  "$basedir/../../../../../r2.cnpmjs.org+escodegen@2.0.0/node_modules/escodegen/bin/esgenerate.js" "$@"
fi
